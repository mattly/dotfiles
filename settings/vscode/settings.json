{
    "[clojure]": {
        "editor.wordSeparators": "\t ()\"':,;~@#$%^&{}[]`-./"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true
        },
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.quickSuggestions": {
            "comments": false,
            "other": false,
            "strings": false
        }
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "breadcrumbs.enabled": true,
    "calva.evalOnSave": true,
    "calva.fmt.newIndentEngine": true,
    "calva.paredit.defaultKeyMap": "strict",
    "calva.prettyPrintingOptions": {
        "printEngine": "pprint",
        "enabled": true,
        "width": 120,
        "maxLength": 50
    },
    "calva.statusColor": {},
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorStyle": "line",
    "editor.find.autoFindInSelection": "multiline",
    "editor.fontFamily": "'Triplicate A Code', 'MesloLGL Nerd Font',  Menlo, monospace",
    "editor.fontLigatures": false,
    "editor.fontSize": 14,
    "editor.fontWeight": "400",
    "editor.formatOnType": true,
    "editor.guides.bracketPairs": true,
    "editor.lineHeight": 0,
    "editor.lineNumbers": "on",
    "editor.matchBrackets": "near",
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "all",
    "editor.showFoldingControls": "always",
    "editor.tokenColorCustomizations": {},
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        ".envrc": "shellscript"
    },
    "files.trimTrailingWhitespace": true,
    "git.allowForcePush": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.fetchOnPull": true,
    "git.path": "/usr/local/bin/git",
    "git.suggestSmartCommit": false,
    "gitlens.advanced.messages": {
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressLineUncommittedWarning": true
    },
    "gitlens.codeLens.scopes": [],
    "gitlens.heatmap.toggleMode": "window",
    "gitlens.keymap": "alternate",
    "gitlens.showWhatsNewAfterUpgrades": false,
    "gitlens.statusBar.reduceFlicker": true,
    "gitlens.views.repositories.files.layout": "auto",
    "html.format.endWithNewline": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "rest-client.fontSize": 13,
    "scm.alwaysShowActions": true,
    "scm.defaultViewMode": "tree",
    "scm.inputFontFamily": "editor",
    "terminal.external.osxExec": "ITerm.app",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontWeightBold": "normal",
    "terminal.integrated.defaultProfile.osx": "fish",
    "terminal.integrated.profiles.osx": {
        "fish": {
            "path": "/opt/homebrew/bin/fish",
            "icon": "terminal",
        }
    },
    "terminal.integrated.profiles.windows": {},
    "terminal.integrated.scrollback": 10000,
    "vim.easymotion": true,
    "vim.foldfix": true,
    "vim.gdefault": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        }
    ],
    "vim.useCtrlKeys": false,
    "vim.useSystemClipboard": true,
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        }
    ],
    "vspacecode.bindingOverrides": [
        {
            "command": "git-graph.view",
            "keys": "g.g",
            "name": "Graph",
            "type": "command"
        },
        {
            "keys": "w.h",
            "position": -1
        },
        {
            "command": "workbench.action.navigateLeft",
            "keys": "w.j",
            "name": "Focus editor left",
            "type": "command"
        },
        {
            "command": "workbench.action.navigateDown",
            "keys": "w.k",
            "name": "Focus editor down",
            "type": "command"
        },
        {
            "command": "workbench.action.navigateUp",
            "keys": "w.i",
            "name": "Focus editor up",
            "type": "command"
        },
        {
            "command": "workbench.action.navigateRight",
            "keys": "w.l",
            "name": "Focus editor right",
            "type": "command"
        },
        {
            "keys": "w.H",
            "position": -1
        },
        {
            "command": "workbench.action.moveActiveEditorGroupLeft",
            "keys": "w.J",
            "name": "Move editor group left",
            "type": "command"
        },
        {
            "command": "workbench.action.moveActiveEditorGroupUp",
            "keys": "w.I",
            "name": "Move editor group up",
            "type": "command"
        },
        {
            "command": "workbench.action.moveActiveEditorGroupDown",
            "keys": "w.K",
            "name": "Move editor group down",
            "type": "command"
        }
    ],
    "window.newWindowDimensions": "inherit",
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {},
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.showTabs": true,
    "workbench.preferredLightColorTheme": "Nüshu",
    "workbench.sideBar.location": "left",
    "workbench.statusBar.visible": true,
    "workbench.view.alwaysShowHeaderActions": true,
    "window.autoDetectHighContrast": false,
    "workbench.colorTheme": "Nüshu",
    "go.autocompleteUnimportedPackages": true,
    "go.formatTool": "gofmt",
    "go.languageServerExperimentalFeatures": {
        "diagnostics": false
    },
    "editor.trimAutoWhitespace": false,
    "openInGitHub.useLocalLine": true,
    "files.exclude": {
        "**/.trunk/*out": true
    },
    "files.watcherExclude": {
        "**/.trunk/*out": true
    },
    "trunk.trunkGrayOutNonBlockingIssues": false,
    "go.toolsManagement.autoUpdate": true,
    "protoc": {
      "path": "protoc",
          "options": [
              "--proto_path=${workspaceRoot}/proto"
          ]
      },
    "editor.codeActionsOnSave": {

    },
    "terminal.integrated.confirmOnExit": "hasChildProcesses",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "magit.forge-enabled": true,
    "gitlens.plusFeatures.enabled": false,
}
