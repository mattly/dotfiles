{
    "[clojure]": {
        "editor.wordSeparators": "\t ()\"':,;~@#$%^&{}[]`-./"
    },
    "bracket-pair-colorizer-2.colors": [
        "#AC362D",
        "#3BA3F8",
        "#E826AE",
    ],
    "bracket-pair-colorizer-2.highlightActiveScope": true,
    "bracket-pair-colorizer-2.showBracketsInGutter": true,
    "bracket-pair-colorizer-2.showBracketsInRuler": true,
    "breadcrumbs.enabled": false,
    "calva.fmt.allgnMapItems": true,
    "calva.fmt.newIndentEngine": true,
    "calva.jokerPath": "",
    "calva.lintOnSave": true,
    "calva.paredit.defaultKeyMap": "strict",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.cursorStyle": "line",
    "editor.find.autoFindInSelection": true,
    "editor.fontFamily": "IBM Plex Mono, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.fontWeight": "400",
    "editor.highlightActiveIndentGuide": true,
    "editor.lineNumbers": "on",
    "editor.matchBrackets": false,
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": false,
    "editor.showFoldingControls": "always",
    "editor.tokenColorCustomizations": {
        "[Outrun Night]": {
            "comments": "#add0e5",
            "strings": "#a6e22e",
            "variables": "#ff2afc"
        },
        "[Quiet Light for VSC]": {
            "comments": "#277A95"
        },
    },
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.path": "/usr/local/bin/git",
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.codeLens.scopes": ["blocks", "containers"],
    "gitlens.heatmap.toggleMode": "window",
    "gitlens.keymap": "alternate",
    "gitlens.views.repositories.files.layout": "auto",
    "markdown.preview.fontFamily": "'IBM Plex Serif', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-serif",
    "search.location": "panel",
    "terminal.external.osxExec": "ITerm.app",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.fontSize": 13,
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "[Alabaster]": {
            "statusBar.background": "#F0F0F0",
            "statusBar.foreground": "#000028",
            "statusBar.noFolderBackground": "#F7F7F7",
            "statusBar.noFolderForeground": "#000028",
        },
        "[Quiet Light]": {
            "statusBar.background": "#AC3543"
        },
        "[Quiet Light for VSC]": {
            "statusBar.background": "#AC3543",
            "statusBar.noFolderBackground": "#277A95"
        },
    },
    "workbench.colorTheme": "Quiet Light for VSC",
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.showTabs": true,
    "workbench.iconTheme": "vscode-great-icons",
    "workbench.statusBar.feedback.visible": false,
    "workbench.statusBar.visible": true,
    "editor.formatOnType": true,
    "terminal.integrated.lineHeight": 1.2,
    "rest-client.fontSize": 13,
    "editor.renderLineHighlight": "all",
    "scm.alwaysShowActions": true,
    "git.fetchOnPull": true
}
