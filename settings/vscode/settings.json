{
    "[clojure]": {
        "editor.wordSeparators": "\t ()\"':,;~@#$%^&{}[]`-./"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true
        },
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.quickSuggestions": {
            "comments": false,
            "other": false,
            "strings": false
        }
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "breadcrumbs.enabled": true,
    "calva.evalOnSave": true,
    "calva.fmt.newIndentEngine": true,
    "calva.paredit.defaultKeyMap": "strict",
    "calva.prettyPrintingOptions": {
        "enabled": true,
        "maxLength": 50,
        "printEngine": "pprint",
        "width": 120
    },
    "calva.statusColor": {},
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.codeActionsOnSave": {},
    "editor.cursorBlinking": "smooth",
    "editor.cursorStyle": "line",
    "editor.find.autoFindInSelection": "multiline",
    "editor.fontFamily": "'Monolisa', 'Dank Mono', 'MesloLGL Nerd Font',  Menlo, monospace",
    "editor.fontLigatures": false,
    "editor.fontSize": 13,
    "editor.fontWeight": "400",
    "editor.formatOnType": true,
    "editor.guides.bracketPairs": true,
    "editor.lineHeight": 0,
    "editor.lineNumbers": "on",
    "editor.matchBrackets": "near",
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "none",
    "editor.showFoldingControls": "always",
    "editor.tokenColorCustomizations": {},
    "editor.trimAutoWhitespace": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        ".envrc": "shellscript"
    },
    "files.exclude": {
        "**/.trunk/*out": true
    },
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.trunk/*out": true
    },
    "git.allowForcePush": true,
    "git.autoRepositoryDetection": "subFolders",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.fetchOnPull": true,
    "git.path": "/usr/local/bin/git",
    "git.suggestSmartCommit": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.currentLine.pullRequests.enabled": false,
    "gitlens.plusFeatures.enabled": false,
    "gitlens.showWelcomeOnInstall": false,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "go.autocompleteUnimportedPackages": true,
    "go.formatTool": "gofmt",
    "go.languageServerExperimentalFeatures": {
        "diagnostics": false
    },
    "go.toolsManagement.autoUpdate": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "magit.display-buffer-function": "same-column",
    "magit.forge-enabled": true,
    "protoc": {
        "options": [
            "--proto_path=${workspaceRoot}/proto"
        ],
        "path": "protoc"
    },
    "rest-client.fontSize": 13,
    "scm.alwaysShowActions": true,
    "scm.defaultViewMode": "tree",
    "scm.inputFontFamily": "editor",
    "terminal.external.osxExec": "ITerm.app",
    "terminal.integrated.confirmOnExit": "hasChildProcesses",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.defaultProfile.osx": "fish",
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontWeightBold": "normal",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.profiles.osx": {
        "fish": {
            "icon": "terminal",
            "path": "/opt/homebrew/bin/fish"
        }
    },
    "terminal.integrated.profiles.windows": {},
    "terminal.integrated.scrollback": 10000,
    "window.autoDetectHighContrast": false,
    "window.commandCenter": false,
    "window.newWindowDimensions": "inherit",
    "window.zoomLevel": 1,
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Bearded Theme Surprising Watermelon",
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.showTabs": true,
    "workbench.iconTheme": "bearded-icons",
    "workbench.sideBar.location": "left",
    "workbench.statusBar.visible": true,
    "workbench.view.alwaysShowHeaderActions": true
}
