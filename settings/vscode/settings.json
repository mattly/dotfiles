{
    "[clojure]": {
        "editor.wordSeparators": "\t ()\"':,;~@#$%^&{}[]`-./"
    },
    "bracketPairColorizer.consecutivePairColors": [
        "()",
        "[]",
        "{}",
        [
            "#D73ABA",
            "#ED3157",
            "#178E84"
        ],
        "Red"
    ],
    "bracketPairColorizer.highlightActiveScope": true,
    "bracketPairColorizer.showBracketsInRuler": true,
    "breadcrumbs.enabled": false,
    "calva.lintOnSave": true,
    "editor.cursorStyle": "line",
    "editor.find.autoFindInSelection": true,
    "editor.fontFamily": "IBM Plex Mono, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.fontWeight": "400",
    "editor.highlightActiveIndentGuide": true,
    "editor.lineNumbers": "on",
    "editor.matchBrackets": false,
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": false,
    "editor.renderLineHighlight": "gutter",
    "editor.showFoldingControls": "always",
    "editor.tokenColorCustomizations": {
        "[Night Owl]": {
            "comments": "#A7C3C3"
        },
        "[Outrun Night]": {
            "comments": "#add0e5",
            "strings": "#a6e22e",
            "variables": "#ff2afc"
        }
    },
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.path": "/usr/local/bin/git",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.heatmap.toggleMode": "window",
    "gitlens.keymap": "alternate",
    "markdown.preview.fontFamily": "'IBM Plex Serif', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-serif",
    "terminal.external.osxExec": "ITerm.app",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1.1,
    "vim.autoindent": true,
    "vim.easymotion": false,
    "vim.easymotionMarkerFontFamily": "IBM Plex Mono",
    "vim.foldfix": true,
    "vim.ignorecase": true,
    "vim.incsearch": true,
    "vim.leader": "space",
    "vim.normalModeKeyBindings": [
        {
            "before": [":"],
            "commands": [
                "workbench.action.showCommands",
            ]
        },
        {
            "before": ["O"],
            "after": [ "k", "A", "\n" ]
        },
        {
            "before": ["o"],
            "after": [ "A", "\n" ]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "g"
            ],
            "commands": [
                "workbench.view.scm"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "x"
            ],
            "commands": [
                "workbench.view.extensions"
            ]
        }
    ],
    "vim.smartcase": true,
    "vim.sneak": true,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "[Night Owl]": {
            "activityBar.background": "#000C1D",
            "activityBar.border": "#102a44",
            "sideBar.background": "#001122",
            "sideBar.border": "#102a44",
            "sideBar.foreground": "#8BADC1"
        },
        "[Quiet Light]": {
            "statusBar.background": "#AC3543"
        }
    },
    "workbench.colorTheme": "Outrun Night",
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.showTabs": true,
    "workbench.iconTheme": "vscode-great-icons",
    "workbench.statusBar.feedback.visible": false,
    "window.zoomLevel": 0,
    "vim.disableExtension": false,
    "gitlens.views.repositories.files.layout": "auto",
    "calva.paredit.defaultKeyMap": "strict",
    "vim.hlsearch": true
}
