{
    "[clojure]": {
        "editor.wordSeparators": "\t ()\"':,;~@#$%^&{}[]`-./"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "bracket-pair-colorizer-2.highlightActiveScope": true,
    "bracket-pair-colorizer-2.showBracketsInGutter": true,
    "bracket-pair-colorizer-2.showBracketsInRuler": true,
    "bracket-pair-colorizer-2.colors": [
        "#888",
    ],
    "breadcrumbs.enabled": true,
    "calva.fmt.alignMapItems": true,
    "calva.fmt.newIndentEngine": true,
    "calva.jokerPath": "",
    "calva.paredit.defaultKeyMap": "strict",
    "calva.statusColor": {},
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.cursorStyle": "line",
    "editor.find.autoFindInSelection": true,
    "editor.fontFamily": "mononoki, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.fontWeight": "400",
    "editor.formatOnType": true,
    "editor.highlightActiveIndentGuide": true,
    "editor.lineNumbers": "on",
    "editor.matchBrackets": false,
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": false,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "selection",
    "editor.showFoldingControls": "always",
    "editor.tokenColorCustomizations": {
        "[Daybreak (Italic)]": {
            "comments": "#ff0"
        },
        "[Quiet Light for VSC]": {
            "comments": "#000",
            "textMateRules": [
                {
                    "scope": [
                        "comment.keyword", "keyword.codetag", "keyword.phpdoc",
                        "entity.name.function", "entity.name.class", "entity.method.name",
                        "entity.name.type", "entity.other.inherited-class", "support.class",
                        "entity.name.section",
                        "keyword.other.important",
                    ],
                    "settings": {"fontStyle": "bold italic"}
                }
            ]
        }
    },
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.associations": {
        ".envrc": "shellscript"
    },
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.fetchOnPull": true,
    "git.path": "/usr/local/bin/git",
    "gitlens.advanced.messages": {
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressLineUncommittedWarning": true,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.codeLens.scopes": [],
    "gitlens.heatmap.toggleMode": "window",
    "gitlens.keymap": "alternate",
    "gitlens.showWhatsNewAfterUpgrades": false,
    "gitlens.statusBar.reduceFlicker": true,
    "gitlens.views.repositories.files.layout": "auto",
    "go.gopath": "/Users/mlyon/projects",
    "go.useLanguageServer": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "prettier.printWidth": 110,
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "prettier.trailingComma": "es5",
    "rest-client.fontSize": 13,
    "scm.alwaysShowActions": true,
    "search.location": "panel",
    "terminal.external.osxExec": "ITerm.app",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1.2,
    "terminal.integrated.rendererType": "dom",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "[Quiet Light for VSC]": {
            "statusBar.background": "#f2f2f2",
            "statusBar.foreground": "#333",
            "statusBar.noFolderBackground": "#e4f6d4",
            "diffEditor.insertedTextBackground": "#4b83cd20",
            "diffEditor.removedTextBackground": "#fef93530",
            "titleBar.activeBackground": "#f2f2f2",
        }
    },
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.showTabs": true,
    "workbench.iconTheme": "file-icons",
    "workbench.sideBar.location": "left",
    "workbench.statusBar.visible": true,
    "workbench.colorTheme": "Daybreak (Italic)",
}
