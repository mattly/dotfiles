{
    "[clojure]": {
        "editor.wordSeparators": "\t ()\"':,;~@#$%^&{}[]`-./"
    },
    "bracket-pair-colorizer-2.colors": [
        "#A4E405",
        "#ED3157",
        "#178E84"
    ],
    "bracket-pair-colorizer-2.highlightActiveScope": true,
    "bracket-pair-colorizer-2.showBracketsInGutter": true,
    "bracket-pair-colorizer-2.showBracketsInRuler": true,
    "breadcrumbs.enabled": false,
    "calva.jokerPath": "",
    "calva.lintOnSave": true,
    "calva.paredit.defaultKeyMap": "strict",
    "editor.cursorStyle": "line",
    "editor.find.autoFindInSelection": true,
    "editor.fontFamily": "Fira Code, Iosevka, IBM Plex Mono, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "editor.fontWeight": "400",
    "editor.highlightActiveIndentGuide": true,
    "editor.lineNumbers": "on",
    "editor.matchBrackets": false,
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": false,
    "editor.renderLineHighlight": "gutter",
    "editor.showFoldingControls": "always",
    "editor.tokenColorCustomizations": {
        "[Night Owl]": {
            "comments": "#A7C3C3"
        },
        "[Outrun Night]": {
            "comments": "#add0e5",
            "strings": "#a6e22e",
            "variables": "#ff2afc"
        }
    },
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.path": "/usr/local/bin/git",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.heatmap.toggleMode": "window",
    "gitlens.keymap": "alternate",
    "gitlens.views.repositories.files.layout": "auto",
    "markdown.preview.fontFamily": "'IBM Plex Serif', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-serif",
    "terminal.external.osxExec": "ITerm.app",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1.1,
    "vim.autoindent": true,
    "vim.disableExtension": false,
    "vim.easymotion": true,
    "vim.easymotionMarkerFontFamily": "Fira Code",
    "vim.foldfix": true,
    "vim.hlsearch": true,
    "vim.ignorecase": true,
    "vim.incsearch": true,
    "vim.leader": "space",
    "vim.normalModeKeyBindings": [
        {
            "before": [":"],
            "commands": [ "workbench.action.showCommands", ]
        },
        {
            "before": ["O"],
            "after": [ "k", "A", "\n" ]
        },
        {
            "before": ["o"],
            "after": [ "A", "\n" ]
        },
        {
            "before": [ "<leader>", "f", "f" ],
            "commands": [ "workbench.action.quickOpen" ]
        },
        {
            "before": [ "<leader>", "f", "s" ],
            "commands": [ "workbench.action.files.save" ]
        },
        {
            "before": [ "<leader>", "g", "g" ],
            "commands": [ "workbench.view.scm" ]
        },
        {
            "before": [ "<leader>", "x", "x" ],
            "commands": [ "workbench.view.extensions" ]
        }
    ],
    "vim.smartcase": true,
    "vim.sneak": true,
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "[Gruvbox Dark (Hard)]": {
            "statusBar.background": "#111213"
        },
        "[Night Owl]": {
            "activityBar.background": "#000C1D",
            "activityBar.border": "#102a44",
            "sideBar.background": "#001122",
            "sideBar.border": "#102a44",
            "sideBar.foreground": "#8BADC1"
        },
        "[Quiet Light]": {
            "statusBar.background": "#AC3543"
        }
    },
    "workbench.colorTheme": "Outrun Night",
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.showTabs": true,
    "workbench.iconTheme": "vscode-great-icons",
    "workbench.statusBar.feedback.visible": false,
    "workbench.statusBar.visible": true,
}
