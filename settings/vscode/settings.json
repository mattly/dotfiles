{
    "[clojure]": {
        "editor.wordSeparators": "\t ()\"':,;~@#$%^&{}[]`-./"
    },
    "editor.fontFamily": "IBM Plex Mono, monospace",
    "editor.fontSize": 14,
    "editor.fontWeight": "normal",
    "editor.highlightActiveIndentGuide": true,
    "editor.lineHeight": 20,
    "editor.lineNumbers": "on",
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": false,
    "editor.tokenColorCustomizations": {
        "[Night Owl]": {
            "comments": "#A7C3C3"
        }
    },
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "git.autofetch": true,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.codeLens.scopes": [],
    "gitlens.heatmap.toggleMode": "window",
    "gitlens.keymap": "alternate",
    "markdown.preview.fontFamily": "'IBM Plex Serif', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-serif",
    "search.location": "panel",
    "window.zoomLevel": 1,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "[Night Owl]": {
            "activityBar.background": "#000C1D",
            "activityBar.border": "#102a44",
            "sideBar.background": "#001122",
            "sideBar.border": "#102a44",
            "sideBar.foreground": "#8BADC1"
        },
        "[Quiet Light]": {
            "statusBar.background": "#AC3543"
        },
        "[CodinCat Lighter]": {
            "statusBar.background": "#9E8C3C",
            "statusBar.noFolderBackground": "#FF7F7D"
        }
    },
    "workbench.colorTheme": "CodinCat Lighter",
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.showTabs": true,
    "workbench.iconTheme": "vscode-great-icons",
    "workbench.statusBar.feedback.visible": false,
    "files.trimTrailingWhitespace": true,
    "editor.renderControlCharacters": true,
    "gitlens.gitExplorer.files.layout": "auto",
    "git.path": "/usr/local/bin/git",
    "editor.matchBrackets": false,
    "bracketPairColorizer.showBracketsInRuler": true,
    "bracketPairColorizer.highlightActiveScope": true,
    "git.confirmSync": false,
    "emmet.showExpandedAbbreviation": "never",
    "editor.renderLineHighlight": "gutter"
}
